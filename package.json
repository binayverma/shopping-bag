{
  "name": "shopping-bag",
  "version": "1.0.0",
  "description": "A Shopping app using Reactjs, Redux, React-Router, ES6, Babel and Webpack.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src",
    "test": "jest",
    "start": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test:watch": "npm test -- --watch",
    "coverage": "jest --coverage",
    "build": "rimraf dist && webpack --config ./webpack.config.js --progress --profile --colors && cp index.html ./dist && cp -R assets dist/assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binayverma/shopping-bag.git"
  },
  "keywords": [
    "Shopping",
    "ReactJs",
    "Javascript",
    "Redux",
    "CSS3",
    "SCSS",
    "ES6",
    "Cart",
    "Shopping Bag"
  ],
  "author": "Binay Verma (binayverma01@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/binayverma/shopping-bag/issues"
  },
  "homepage": "https://github.com/binayverma/shopping-bag#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.10.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.4.0",
    "node-sass": "^4.6.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "babel-preset-stage-1": "^6.24.1",
    "bootstrap": "^3.3.7",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "lodash": "^4.17.4",
    "react": "15.6.0",
    "react-dom": "15.6.0",
    "react-modal-bootstrap": "1.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.1.2",
    "redux-promise": "^0.5.3",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2"
  }
}
